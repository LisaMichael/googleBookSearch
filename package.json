    "axios": "^0.18.0",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "mini-create-react-context": "^0.3.2",
    "mongoose": "^5.3.16",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "save": "^2.4.0"
  },
