  },
  "homepage": "https://github.com/LisaMichael/googleBookSearch#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "mongoose": "^5.9.1"
  },
  "devDependencies": {}
}
